    <!-- http://linkedart.ascdc.tw/lod/Painting/AW0156 -->

    <owl:NamedIndividual rdf:about="http://linkedart.ascdc.tw/lod/Painting/AW0156">
        <rdf:type rdf:resource="http://linkedart.ascdc.tw/lod/ontology#Painting"/>
        <crm:P2_has_type rdf:resource="http://linkedart.ascdc.tw/lod/Subject/SB0054"/>
