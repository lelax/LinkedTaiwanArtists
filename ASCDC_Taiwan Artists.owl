    <!-- http://linkedart.ascdc.tw/lod/Photography/PH0035 -->

    <owl:NamedIndividual rdf:about="http://linkedart.ascdc.tw/lod/Photography/PH0035">
        <rdf:type rdf:resource="http://linkedart.ascdc.tw/lod/ontology#Photograph"/>
        <crm:P62_depicts rdf:resource="http://linkedart.ascdc.tw/lod/Person/PR0001"/>
        <crm:P62_depicts rdf:resource="http://linkedart.ascdc.tw/lod/Person/PR0111"/>
        <crm:P62_depicts rdf:resource="http://linkedart.ascdc.tw/lod/Person/PR0204"/>
