        <rdfs:label xml:lang="zh">戰災</rdfs:label>
        <skos:altLabel xml:lang="en">Beside the Commercial Press Building</skos:altLabel>
        <skos:altLabel xml:lang="zh">商務印書館側</skos:altLabel>
    </owl:NamedIndividual>
    


