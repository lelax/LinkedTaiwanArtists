    <owl:NamedIndividual rdf:about="http://linkedart.ascdc.tw/lod/Painting/AW0045">
        <rdf:type rdf:resource="http://linkedart.ascdc.tw/lod/ontology#Painting"/>
        <crm:P12_was_present_at rdf:resource="http://linkedart.ascdc.tw/lod/Exhibition/EX0010-004"/>
        <crm:P2_has_type rdf:resource="http://linkedart.ascdc.tw/lod/Subject/SB0085"/>
