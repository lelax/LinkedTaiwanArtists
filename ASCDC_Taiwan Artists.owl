        <schema.org:award xml:lang="zh">無鑑查</schema.org:award>
        <schema.org:dateCreated rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1938</schema.org:dateCreated>
        <rdfs:label xml:lang="en">Ancient Temple</rdfs:label>
        <rdfs:label xml:lang="zh">古廟</rdfs:label>
    </owl:NamedIndividual>
    


